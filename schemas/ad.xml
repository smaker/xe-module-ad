<table name="ad">
	<column name="document_srl" type="number" size="11" notnull="notnull" primary_key="primary_key" />
	<column name="module_srl" type="number" size="11" notnull="notnull" />
	<column name="start_date" type="date" index="idx_start_date" />
	<column name="end_date" type="date" index="idx_end_date" />
	<column name="ad_time" type="number" size="11" index="idx_ad_time" />
	<column name="ad_type" type="varchar" size="20" default="linead" index="idx_ad_type" />
	<column name="url" type="text" />
	<column name="url_target" type="varchar" size="11" />
	<!-- Publish Status (wait|published)-->
	<column name="publish_status" type="varchar" size="9" default="published" />
	<column name="publish_date" type="date" />
	<column name="style" type="text" />
	<column name="extra_vars" type="text" />
</table>