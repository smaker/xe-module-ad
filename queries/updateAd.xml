<query id="updateAd" action="update">
	<tables>
		<table name="ad" />
	</tables>
	<columns>
		<column name="module_srl" var="module_srl" />
		<column name="start_date" var="start_date" />
		<column name="end_date" var="end_date" />
		<column name="ad_time" var="ad_time" />
		<column name="ad_type" var="ad_type" default="linead" />
		<column name="publish_status" var="publish_status" />
		<column name="publish_date" var="publish_date" />
		<column name="url" var="url" />
		<column name="url_target" var="url_target" default="_blank" />
		<column name="style" var="style" />
		<column name="extra_vars" var="extra_vars" />
	</columns>
	<conditions>
		<condition operation="equal" column="document_srl" var="document_srl" filter="number" notnull="notnull" />
	</conditions>
</query>