<query id="updateOldModule" action="update">
	<tables>
		<table name="modules" />
	</tables>
	<columns>
		<column name="module" var="new_module" notnull="notnull" maxlength="80" />
	</columns>
	<conditions>
		<condition operation="equal" column="module" var="old_module" default="linead" />
	</conditions>
</query>
